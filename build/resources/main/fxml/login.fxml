<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="440.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Login.LoginController">
   <children>
      <TextField id="username" fx:id="username" layoutX="206.0" layoutY="146.0" promptText="Username" />
      <PasswordField fx:id="password" layoutX="206.0" layoutY="182.0" promptText="Password" />
      <Button fx:id="loginButton" layoutX="248.0" layoutY="220.0" mnemonicParsing="false" onAction="#loginButtonClicked" text="Login" />
   </children>
</AnchorPane>
